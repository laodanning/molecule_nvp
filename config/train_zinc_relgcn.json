{
    "dataset":
    {
        "root_dir": "./dataset",
        "name": "zinc250k_relgraph_kekulized.npz"
    },
    "configuration":
    {
        "root_dir": "./config",
        "atom_id_to_atomic_num": "atomic_num_zinc.json",
        "train_validation_split": "validation_idx_zinc.json"
    },
    "output":
    {
        "root_dir": "./output/zinc",
        "trainlogname": "trainlog",
        "saved_mol_dir": "result_mol",
        "log_level": "info",
        "final_model_name": "molecule_nvp_relgcn.npz",
        "logname": "log",
        "hyperparams_name": "hyperparams_relgcn.json"
    },
    "model":
    {
        "embed_model_path": "./output/zinc/final_embed_model.npz",
        "embed_model_hyper": "./output/zinc/atom_embed_model_hyper.json",
        "num_edge_types": 4,
        "num_features": 10,
        "num_nodes": 38,
        "apply_batchnorm": true,
        "num_coupling":
        {
            "feature": 40,
            "relation": 40
        },
        "gnn_type": "relgcn",
        "gnn_params": {
            "ch_list": [16, 128],
            "scale_adj": true
        },
        "gnn_fc_channels": [256, 64],
        "learn_dist": true,
        "mlp_channels": [1024, 512],
        "additive_feature_coupling": false,
        "additive_relation_coupling": false,
        "feature_noise_scale": 0.01,
        "initial_z_var": 0.1
    },
    "train":
    {
        "device": 0,
        "batch_size": 1024,
        "num_epoch": 200,
        "save_epoch": 10,
        "optimizer": "adam",
        "two_step": true,
        "h_nll_weight": 1
    }
}