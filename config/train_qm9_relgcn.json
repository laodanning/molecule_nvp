{
    "dataset":
    {
        "root_dir": "./dataset",
        "name": "qm9_relgraph_kekulized.npz"
    },
    "configuration":
    {
        "root_dir": "./config",
        "atom_id_to_atomic_num": "atomic_num_qm9.json",
        "train_validation_split": "validation_idx_qm9.json"
    },
    "output":
    {
        "root_dir": "./output/qm9",
        "trainlogname": "trainlog",
        "saved_mol_dir": "result_mol",
        "log_level": "info",
        "final_model_name": "molecule_nvp_relgcn.npz",
        "logname": "log",
        "hyperparams_name": "hyperparams_relgcn.json"
    },
    "model":
    {
        "molecule_nvp_params": 
        {
            "max_atom_types": 5,
            "embed_model_path": "./output/qm9/final_embed_model.npz",
            "embed_model_hyper": "./output/qm9/atom_embed_model_hyper.json",
            "num_edge_types": 4,
            "num_features": 5,
            "num_nodes": 9,
            "apply_batchnorm": true,
            "num_coupling":
            {
                "feature": 36,
                "relation": 27
            },
            "gnn_type": "relgcn",
            "gnn_params": {
                "ch_list": [16, 64],
                "scale_adj": true
            },
            "gnn_fc_channels": [128, 64],
            "learn_dist": true,
            "mlp_channels": [256, 256],
            "additive_feature_coupling": false,
            "additive_relation_coupling": false,
            "feature_noise_scale": 0.001,
            "initial_z_var": 1,
            "latent_coupling_layers": [4, 4, 4, 4, 4, 4],
            "latent_coupling_ch_list": 
            {
                "x": [128, 64],
                "adj": [256, 256]
            },
            "apply_shuffle": true,
            "num_shuffle_blocks": 0,
            "block_params":
            {
                "num_x_couplings": 2,
                "num_A_couplings": 2,
                "x_ch_list": [128, 64],
                "A_ch_list": [256, 256]
            }
        },
        "latent_nvp_params": 
        {
            "channel_size": 396,
            "ch_list": [256, 256],
            "learn_var": true,
            "num_coupling_layers": 10
        }
    },
    "train":
    {
        "device": 0,
        "batch_size": 512,
        "num_epoch": 50,
        "second_num_iter": 1000,
        "second_check_every": 100,
        "save_epoch": 10,
        "optimizer": "adam",
        "two_step": true,
        "h_nll_weight": 1
    }
}