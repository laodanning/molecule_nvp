{
    "dataset":
    {
        "root_dir": "./dataset",
        "name": "qm9_relgraph_kekulized.npz"
    },
    "configuration":
    {
        "root_dir": "./config",
        "atom_id_to_atomic_num": "atomic_num_qm9.json",
        "train_validation_split": "validation_idx_qm9.json"
    },
    "output":
    {
        "root_dir": "./output/qm9",
        "logname": "trainlog"
    },
    "model":
    {
        "embed_model_path": "./output/qm9/final_embed_model.npz",
        "embed_model_hyper": "./output/qm9/atom_embed_model_hyper.json",
        "num_edge_type": 4,
        "num_feature": 8,
        "num_node": 9,
        "apply_batchnorm": true,
        "num_coupling":
        {
            "feature": 8,
            "relation": 4
        },
        "num_attention_types": 4,
        "num_gat_layers": 4,
        "gnn_channels": [8, 8],
        "mlp_channels": [8, 8]
    },
    "training":
    {
        "device": 0,
        "batch_size": 32,
        "num_epoch": 2,
        "save_epoch": 1,
        "optimizer": "adam",
        "optimizer_params":
        {
            "learning_rate": 1e-4
        },
        "two_step": true,
        "adj_nll_weight": 1.0
    }
}